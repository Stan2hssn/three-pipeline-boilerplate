{
  "name": "pipeline_template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "assets:migrate": "bash scripts/migrate-assets-structure.sh",
    "assets:convert": "./scripts/convert-assets-for-prod.sh",
    "assets:convert:clean": "CLEANUP_ORIGINALS=true ./scripts/convert-assets-for-prod.sh",
    "assets:convert:high-quality": "WEBP_QUALITY=95 WEBM_CRF=28 ./scripts/convert-assets-for-prod.sh",
    "assets:generate-types": "node scripts/generate-assets-types.js",
    "assets:all": "npm run assets:convert && npm run assets:generate-types",
    "prebuild": "npm run assets:convert",
    "postbuild": "node scripts/prune-dist-assets-source.mjs"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/three": "^0.181.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.33.5",
    "three-perf": "^1.0.11",
    "stats-gl": "^3.8.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "dependencies": {
    "@tweakpane/core": "^2.0.5",
    "gsap": "^3.13.0",
    "postprocessing": "^6.37.8",
    "three": "^0.181.2",
    "tweakpane": "^4.0.5"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  }
}
